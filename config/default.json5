{
  // Libp2p config
  libp2p: {
    addresses: {
      listen: [
        "/ip4/127.0.0.1/tcp/0"
      ]
    },
    config: {
      peerDiscovery: {
        bootstrap: {
          enabled: false
        }
      }
    }
  },

  // Rooms to subscribe to, strings
  rooms: [],
  // Port the GRPC server is listening to
  grpcPort: 50051,
  generatePeerWithSecp256k1Keys: true,
  displayPeerId: true,

  authorization: {
    expiresIn: '1h',
    secret: '',
    challengeSize: 32
  },

  log: {
    level: "debug",
    filter: null,
    path: null
  }
}
