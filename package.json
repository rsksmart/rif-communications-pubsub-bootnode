    "libp2p": "^0.28.10",
    "peer-id": "^0.14.0"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "tasegir": "^1.8.0"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
