{
  "name": "@rsksmart/rif-communications-pubsub-bootnode",
  "version": "0.1.0-dev.1",
  "description": "Simple bootnode for RIF Communications PubSub",
  "keywords": [
    "RIF",
    "Communications",
    "PubSub",
    "bootnode"
  ],
  "homepage": "https://github.com/rsksmart/rif-communications-pubsub-bootnode#readme",
  "bugs": {
    "url": "https://github.com/rsksmart/rif-communications-pubsub-bootnode/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Vojtech Simetka",
    "email": "vojtech@simetka.cz"
  },
  "files": [
    "config",
    "dist",
    "lib",
    "types"
  ],
  "main": "lib/index.js",
  "bin": {
    "rif-communications-pubsub-bootnode": "./bin/run"
  },
  "repository": "https://github.com/rsksmart/rif-communications-pubsub-bootnode",
  "scripts": {
    "compile": "tasegir compile",
    "compile:watch": "tasegir compile -- --watch",
    "types-check": "tasegir types-check",
    "api-server": "tasegir run --watch ./src/api-server.ts",
    "api-client": "tasegir run --watch ./src/api-client.ts ",
    "api-server-debug": "tasegir run --watch --inspect --inspect-brk ./src/api-server.ts",
    "create-key": "tasegir run --watch ./src/key-utils.ts ",
    "start": "tasegir run --watch ./src/index.ts",
    "generate-peerid": "tasegir run ./src/generate-peerid.ts",
    "lint": "tasegir lint",
    "release": "tasegir release --no-docs --no-build --no-test",
    "build": "tasegir build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "cross-env ALLOW_CONFIG_MUTATIONS=true tasegir test --target node --files ./test/integration/*.spec.ts --files ./test/integration/**/**/*.spec.ts",
    "test:unit": "cross-env ALLOW_CONFIG_MUTATIONS=true tasegir test --target node --files ./test/unit/*.spec.ts --files ./test/unit/**/*.spec.ts --files ./test/unit/**/**/*.spec.ts",
    "tasegir:link": "ln -s ./node_modules/tasegir/src/config/tsconfig.json ./tsconfig.json && ln -s ./node_modules/tasegir/src/config/eslintrc.js ./.eslintrc.js"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.5",
    "@lifeomic/attempt": "^3.0.0",
    "@rsksmart/rif-communications-pubsub": "0.1.0-dev.10",
    "@types/asn1js": "0.0.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-forge": "^0.9.5",
    "@types/pkijs": "0.0.5",
    "asn1js": "^2.0.26",
    "chalk": "^4.1.0",
    "color": "^3.1.3",
    "config": "^3.3.2",
    "ethers": "^5.0.24",
    "grpc": "^1.24.3",
    "jsonwebtoken": "^8.5.1",
    "karma": "^5.2.2",
    "key-encoder": "^2.0.3",
    "libp2p": "^0.29.0",
    "libp2p-crypto-secp256k1": "^0.4.3",
    "node-forge": "^0.9.2",
    "node-webcrypto-ossl": "^2.1.2",
    "peer-id": "^0.14.2",
    "pkijs": "^2.1.90",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/dirty-chai": "^2.0.2",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^9.0.3",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "dirty-chai": "^2.0.1",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "tasegir": "1.8.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "types": "types/index.d.ts",
  "contributors": [
    "Vojtech Simetka <vojtech@simetka.cz>",
    "Adam Uhlíř <adam@uhlir.dev>",
    "Raul Laprida <raul@iovlabs.org>"
  ]
}
